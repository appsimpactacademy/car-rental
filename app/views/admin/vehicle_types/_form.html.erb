<%= form_with(model: [:admin, @vehicle_type]) do |form| %>
  
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control mt-2 mb-2", placeholder: "Enter Vehicle Type" %>
  </div>

  <div class="actions">
    <%= form.submit class: "btn btn-primary mt-1 mb-1" %>
    <%= link_to 'Back', admin_vehicle_types_path, class: 'btn btn-warning mt-1 mb-1' %>
  </div>
<% end %>
