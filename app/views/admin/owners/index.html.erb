<div class="row">
  <div class="col-lg-6">
    <h1 class="fw-bold mt-2 mb-3">Owners</h1>
  </div>
</div>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Contact Number</th>
    </tr>
  </thead>
  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.name %></td>
        <td><%= owner.email %></td>
        <td><%= owner.phone_number %></td>
      </tr>
    <% end %>
  </tbody>
</table>
