<%= form_with(model: [:admin, @owner]) do |form| %>
  
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control mt-2 mb-2", placeholder: "Enter Owner" %>
  </div>

  <div class="form-group">
    <%= form.label :email %>
    <%= form.email_field :email, class: "form-control mt-2 mb-2", placeholder: "Enter Email" %>
  </div>

  <div class="form-group">
    <%= form.label :contact_number %>
    <%= form.text_field :contact_number, class: "form-control mt-2 mb-2", placeholder: "Enter Contact Number" %>
  </div>

  <div class="form-group">
    <%= form.label :city %>
    <%= form.text_field :city, class: "form-control mt-2 mb-2", placeholder: "Enter City" %>
  </div>

  <div class="form-group">
    <%= form.label :state %>
    <%= form.text_field :state, class: "form-control mt-2 mb-2", placeholder: "Enter State" %>
  </div>

  <div class="form-group">
    <%= form.label :country %>
    <%= form.text_field :country, class: "form-control mt-2 mb-2", placeholder: "Enter Country" %>
  </div>

  <div class="actions">
    <%= form.submit class: "btn btn-primary mt-1 mb-1" %>
    <%= link_to 'Back', admin_owners_path, class: 'btn btn-warning mt-1 mb-1' %>
  </div>
<% end %>
